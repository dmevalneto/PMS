@model PMS.Models.Secretaria

<div class="content-heading">
    <div>
        <button class="mb-1 btn btn-secondary" type="button">@Html.ActionLink("Voltar", "OperacoesTerceirizado")</button>
    </div>
</div>


<!-- START row-->
<div class="row">
    <div class="col-xl-12">
        <!-- START card-->
        <div class="card card-default">
            <div class="card-header">Terceirizado lotada na Unidade   |  -  @Html.DisplayFor(model => model.Nome)  </div>
            <div class="card-body">
                <br />
                <h4>Setor</h4>
                <address>
                    <strong>Setor</strong>
                    <br />@Html.DisplayFor(model => model.Setor)
                    <br />
                    <abbr title="Phone">Obs:</abbr>  @Html.DisplayFor(model => model.Observacao)
                </address>
            </div>
        </div>
        <!-- END card-->
