@model IEnumerable<PMS.Models.HistoricoStatusOcorrenciaPB>




<div class="card card-default">
    <div class="card-header">@Html.ActionLink("Voltar", "Index")</div>
    <!-- START table-responsive-->
    <div class="table-responsive">
        <table class="table table-bordered table-hover" id="table-ext-1">
            <thead>
                <tr>
                    <th>
                        @Html.DisplayNameFor(model => model.StatusOcorrenciaPB.Status)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.Data)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.Observacao)
                    </th>
                    <th></th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                    <tr>
                        <td>
                            @Html.DisplayFor(modelItem => item.StatusOcorrenciaPB.Status)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Data)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Observacao)
                        </td>
                        <td>
                            @Html.ActionLink("Edit", "Edit", new { id = item.HistoricoStatusOcorrenciaPBId }) |
                            @Html.ActionLink("Details", "Details", new { id = item.HistoricoStatusOcorrenciaPBId }) |
                            @Html.ActionLink("Delete", "Delete", new { id = item.HistoricoStatusOcorrenciaPBId })
