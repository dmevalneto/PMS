@model IEnumerable<PMS.Models.PrefeituraBairro>




<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Nome)
        </th>
       
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        
        <td>
            @Html.DisplayFor(modelItem => item.Nome)
        </td>
        <td>
            @Html.ActionLink("Selecione", "SelecionarOcorrencia", new { id=item.PrefeituraBairroId }) 
        </td>
    </tr>
}

</table>
